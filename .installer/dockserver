# shellcheck disable=SC2086
# shellcheck disable=SC2006
if [[ $EUID != 0 ]]; then
    sudo "$0" "$@"
    exit $?
fi

source /opt/dockserver/function/function.sh

DOCK=$1
APP=${@:2}

case "$DOCK" in
 "" ) run ;;
 "-i" ) run ;;
 "--install" ) run ;;
 "-u" ) update ;;
 "--update" ) update ;;
 "-a" ) fastapp ;;
 "--app" ) fastapp ;;
 "-h" ) usage ;;
 "--help" ) usage ;;

esac

